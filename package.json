{
  "name": "jsdoc-ts-utils",
  "description": "A plugin with utilities to make your TypeScript-like docs JSDoc valid",
  "homepage": "https://github.com/homer0/jsdoc-ts-utils",
  "version": "3.1.0",
  "repository": "homer0/jsdoc-ts-utils",
  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "license": "MIT",
  "keywords": [
    "jsdoc",
    "typescript",
    "ts",
    "documentation",
    "typedef"
  ],
  "dependencies": {
    "jsdoc": "^3.6.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@homer0/eslint-plugin": "^9.1.3",
    "@homer0/prettier-config": "^1.1.2",
    "@homer0/prettier-plugin-jsdoc": "^5.1.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "docdash": "homer0/docdash#semver:^2.1.0",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^29.2.2",
    "leasot": "^13.2.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "main": "src/index.js",
  "scripts": {
    "docs": "./utils/scripts/docs",
    "lint": "./utils/scripts/lint",
    "lint:all": "./utils/scripts/lint-all",
    "test": "./utils/scripts/test",
    "todo": "./utils/scripts/todo",
    "prepare": "./utils/scripts/prepare"
  }
}
